{
  "name": "cypress",
  "version": "1.0.0",
  "description": "Sportsbook E2E project",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "e2e": "cypress run",
    "cy:open": "cypress open",
    "cy:nigeria_prod": "cypress open --config-file config/ng.js -e target_env=PRD,target_tenant=NG,grepTags=bet9ja+prod,grepOmitFiltered=true",
    "cy:nigeria_prod_run": "cypress run --config-file config/ng.js -e target_env=PRD,target_tenant=NG,grepTags=bet9ja+prod,grepOmitFiltered=true",
    "cy:nigeria_stg": "cypress open --config-file config/ng.js -e target_env=STG,target_tenant=NG,grepTags=bet9ja+stg,grepOmitFiltered=true",
    "cy:nigeria_stg_run": "cypress run --config-file config/ng.js -e target_env=STG,target_tenant=NG,grepTags=bet9ja+stg,grepOmitFiltered=true"
  },
